l=[]
n=int(input("enter a number"))
for i in range(0,n,1):
    x=int(input())
    l.append(x)
print(l)
maximum=l[0]
minimum=l[0]
for i in range(1,n):
    if(l[i]>maximum):
        maximum=l[i]
    if(l[i]<minimum):
        minimum=l[i]
print(maximum)
print(minimum)
a=maximum-minimum
print("Difference = ",a)
